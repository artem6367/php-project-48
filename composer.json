{
    "name": "hexlet/code",
    "type": "project",
    "scripts": {
        "test": "phpunit tests"
    },
    "autoload": {
        "psr-4": {
            "hexlet\\code\\": "src/"
        },
        "files": [
            "src/functions.php",
            "src/Parsers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "hexlet\\code\\tests\\": "tests/"
        }
    },
    "require": {
        "phpstan/phpstan": "^1.10",
        "docopt/docopt": "^1.0",
        "symfony/yaml": "^5.4"
    },
    "require-dev": {
        "psy/psysh": "^0.12.0",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.8"
    }
}
